{
  "name": "stripe-payment-connector",
  "version": "0.0.1",
  "private": true,
  "description": "",
  "author": "Axinom",
  "license": "MIT",
  "keywords": [
    "axinom",
    "mosaic",
    "axinom mosaic"
  ],
  "main": "dist/index.js",
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn clean && tsc --project tsconfig.build.json",
    "start": "yarn util:load-vars node dist/index",
    "dev": "concurrently -n \"cpc-ts,stripe-tunnel\" -c \"bgBlue.bold,bgMagenta.bold\" \"yarn dev:ts\" \"yarn dev:tunnel\"",
    "dev:ts": "yarn util:load-vars tsc-watch --project tsconfig.build.json --onSuccess \"yarn node dist/index\"",
    "dev:tunnel": "stripe listen --forward-to localhost:20001/webhooks",
    "debug": "yarn util:load-vars tsc-watch --project tsconfig.build.json --onSuccess \"node --inspect dist/index.js\"",
    "test": "yarn util:load-vars jest --silent",
    "test:watch": "yarn test --watch",
    "test:debug": "yarn util:load-vars node --inspect -r tsconfig-paths/register -r ts-node/register ../../../node_modules/jest/bin/jest --runInBand --silent",
    " -- utilities -- ": "",
    "util:load-vars": "env-cmd -f .env env-cmd -f ../../../.env",
    "codegen": "yarn util:load-vars graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@axinom/mosaic-id-guard": "0.14.11",
    "@axinom/mosaic-id-link-be": "0.8.1",
    "@axinom/mosaic-service-common": "0.23.0",
    "ajv": "^8.11.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "env-cmd": "^10.1.0",
    "env-var": "^7.3.0",
    "express": "^4.17.1",
    "graphile-build": "^4.12.1",
    "graphile-build-pg": "^4.12.1",
    "graphile-utils": "4.12.3",
    "graphql": "^15.8.0",
    "graphql-request": "^5.0.0",
    "graphql-tag": "^2.12.6",
    "node-cache": "^5.1.2",
    "stripe": "^10.12.0-beta.1"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.7.0",
    "@graphql-codegen/typescript": "^2.6.0",
    "@graphql-codegen/typescript-graphql-request": "^4.5.5",
    "@graphql-codegen/typescript-operations": "^2.4.3",
    "@types/amqplib": "^0.8.2",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/jest-expect-message": "^1.0.1",
    "@types/mock-req-res": "^1.1.1",
    "@types/node": "latest",
    "@types/rascal": "^10.0.6",
    "@types/uuid": "^8.3.0",
    "@types/verror": "^1.10.5",
    "@types/yargs": "^16",
    "concurrently": "^7.2.2",
    "jest": "26.6.0",
    "jest-auto-stub": "^1.0.7",
    "jest-expect-message": "^1.0.2",
    "jest-extended": "^0.11.5",
    "mock-req-res": "^1.1.4",
    "rimraf": "^3.0.2",
    "sinon": "^9.0.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^8.9.0",
    "tsc-watch": "^4.2.3",
    "typescript": "^4.5.4"
  }
}
